{
    "compilerOptions": {
        "module": "commonjs",
        "noImplicitAny": true,
        "moduleResolution": "node",
        "target": "es2017",
        "sourceMap": true,
    },
    "include": [
        "src/**/*.ts"
    ],
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "suppressImplicitAnyIndexErrors": true,
    "noUnusedLocals": true
}